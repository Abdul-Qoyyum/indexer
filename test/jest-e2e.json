{
  "moduleFileExtensions": ["js", "json", "ts"],
  "rootDir": ".",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "testPathIgnorePatterns": [
    "/node_modules/",
    "/dist/"
  ],
  "transformIgnorePatterns": [
    "<rootDir>/node_modules/",
    "<rootDir>/dist/"
  ]
}
